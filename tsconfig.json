{
  "extends": "./node_modules/@microsoft/rush-stack-compiler-4.7/includes/tsconfig-web.json",
  "compilerOptions": {
    // "target": "es5",
    "target": "es2018",
    "noImplicitAny": true,
    // "target": "es2015", // or later version
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "declaration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "lib",
    "inlineSources": false,
    "strictNullChecks": false,
    "noUnusedLocals": false,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@microsoft"
    ],
    "types": [
      // "es6-promise",
      "webpack-env"
    ],
    // "lib": [
    //   "es5",
    //   "es2015", // new
    //   "es2017",
    //   "dom",
    //   "es2015.collection"
    // ]
    "lib": [
      "es2018",
      "dom"
    ],
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  // "exclude": [
  //   "node_modules",
  //   "lib"
  // ]
}